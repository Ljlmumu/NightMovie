<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffdadada" android:endColor="#ffdadada" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffdadada" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#fff27b8c" android:endColor="#fff27b8c" android:angle="270.0" android:centerY="0.75" android:centerColor="#fff27b8c" />
            </shape>
        </clip>
    </item>
</layer-list>